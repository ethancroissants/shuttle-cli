{
	"name": "shuttle-ai",
	"version": "1.0.0",
	"description": "ShuttleAI CLI - Autonomous coding agent powered by ShuttleAI. Based on Cline (github.com/cline/cline).",
	"main": "dist/lib.mjs",
	"types": "dist/lib.d.ts",
	"bin": {
		"shuttle": "./dist/cli.mjs"
	},
	"exports": {
		".": {
			"import": "./dist/lib.mjs",
			"types": "./dist/lib.d.ts"
		}
	},
	"os": [
		"darwin",
		"linux",
		"win32"
	],
	"cpu": [
		"x64",
		"arm64"
	],
	"man": "./man/shuttle.1",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"build": "npx tsx esbuild.mts --no-wasm",
		"build:production": "npx tsx esbuild.mts --production",
		"build:fast": "npx tsx esbuild.mts --no-wasm",
		"watch": "npx tsx esbuild.mts --watch --no-wasm",
		"dev": "IS_DEV=true npm run watch",
		"clean": "rimraf dist",
		"typecheck": "npx tsc --noEmit",
		"link": "npm run build && npm link",
		"unlink": "npm unlink -g shuttle",
		"test": "vitest",
		"test:run": "vitest run"
	},
	"keywords": [
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"cli"
	],
	"author": {
		"name": "ShuttleAI Contributors"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/shuttleai/shuttle-cli"
	},
	"homepage": "https://shuttleai.com",
	"bugs": {
		"url": "https://github.com/shuttleai/shuttle-cli/issues"
	},
	"devDependencies": {
		"@types/marked": "^5.0.2",
		"@types/node": "20.x",
		"@types/prompts": "^2.4.9",
		"@types/react": "^19.2.9",
		"dotenv": "^16.4.5",
		"esbuild": "^0.25.0",
		"ink-testing-library": "^4.0.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.4.5",
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"@agentclientprotocol/sdk": "^0.13.1",
		"@anthropic-ai/sdk": "^0.78.0",
		"@google/genai": "^1.43.0",
		"@mistralai/mistralai": "^1.14.1",
		"@modelcontextprotocol/sdk": "^1.27.1",
		"@vscode/ripgrep": "^1.15.9",
		"aws4fetch": "^1.0.20",
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"ink": "npm:@jrichman/ink@6.4.7",
		"ink-picture": "^1.3.3",
		"ink-spinner": "^5.0.0",
		"marked": "^17.0.3",
		"nanoid": "^5.1.6",
		"openai": "^6.25.0",
		"ora": "^8.0.1",
		"pino": "^10.0.0",
		"pino-roll": "^4.0.0",
		"prompts": "^2.4.2",
		"react": "^19.2.3"
	}
}